@startuml
class User {
    -id : Long
    -username : String
    -password : String
    -userRole : UserRole
    -locked : Boolean
    -enabled : Boolean
}

class Person {
    -id : Long
    -name : String
    -dateOfBirth : DateTime
    -email : String
    -phone : String
}

class Admin {

}

class Teacher {

}

class Student {
    -parentId : Long
    -teacherId : Long
}

class Parent {

}

enum UserRole {
    USER, ADMIN
}

Person <|-- Admin
Person <|-- Teacher
Person <|-- Parent
Person <|-- Student
UserRole <-- User

@enduml